{
  "name": "CloudBackup",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "rmbuild": " rm -r ./android/app/build",
    "start": "react-native start",
    "set-realm": "adb forward tcp:8083 tcp:8083",
    "android": "yarn set-realm && react-native run-android",
    "build:android": "react-native run-android --variant=release",
    "test": "jest",
    "scanner:emulator": "adb shell \"find /sdcard/ | while read f; do am broadcast -a android.intent.action.MEDIA_SCANNER_SCAN_FILE -d \"file://${f}\"; done\"",
    "lint": "eslint --ext=js .",
    "precommit": "yarn lint",
    "commit": "git commit",
    "addco": "git add ."
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "dependencies": {
    "appcenter": "^1.5.0",
    "appcenter-analytics": "^1.5.0",
    "appcenter-crashes": "^1.5.0",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "immutable": "^3.8.2",
    "prop-types": "latest",
    "react": "16.3.1",
    "react-native": "0.55.3",
    "react-native-background-task": "^0.2.1",
    "react-native-background-upload": "^4.4.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fs": "^2.9.12",
    "react-native-permissions": "^1.1.1",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.11",
    "react-redux": "^5.0.7",
    "realm": "^2.2.20",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "rxjs": "^5.5.10",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "22.4.3",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
